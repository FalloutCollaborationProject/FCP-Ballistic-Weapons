    <!-- Lucky -->

    <ThingDef ParentName="BaseGun">
        <defName>FCP_Gun_Lucky</defName>
        <label>Lucky</label>
        <description>Lucky is a legendary .357 Magnum revolver, distinguished by its immaculate chrome finish and ornate engravings. Renowned for its flawless action and uncanny accuracy, this weapon is favored by gunslingers who trust in both skill and fortune to carry the day.</description>
        <graphicData>
            <texPath>Weapons/Unique/FCP_Lucky</texPath>
            <graphicClass>Graphic_Single</graphicClass>
        </graphicData>
        <statBases>
            <Mass>1.0</Mass>
            <AccuracyTouch>0.75</AccuracyTouch>
            <AccuracyShort>0.85</AccuracyShort>
            <AccuracyMedium>0.80</AccuracyMedium>
            <AccuracyLong>0.65</AccuracyLong>
            <RangedWeapon_Cooldown>0.9</RangedWeapon_Cooldown>
        </statBases>
        <smeltable>false</smeltable>
        <verbs>
            <li>
                <verbClass>Verb_Shoot</verbClass>
                <hasStandardCommand>true</hasStandardCommand>
                <defaultProjectile>FCP_Bullet_357mm</defaultProjectile>
                <warmupTime>0.5</warmupTime>
                <range>22</range>
                <burstShotCount>1</burstShotCount>
                <soundCast>FCP_Sound_Shot_357_Revolver</soundCast>
                <soundCastTail>GunTail_Medium</soundCastTail>
                <muzzleFlashScale>5</muzzleFlashScale>
            </li>
        </verbs>
        <comps>
            <li Class="FCP.Core.CompProperties_LabelColored">
                <rarity>Unique</rarity>
            </li>
        </comps>
        <modExtensions>
            <li Class="FCP.Core.UniqueThingExtension" />
        </modExtensions>
    </ThingDef>

    <!-- Maria -->

    <ThingDef ParentName="BaseGun">
        <defName>FCP_Gun_Maria</defName>
